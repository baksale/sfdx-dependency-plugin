{
  "name": "sfdx-dependency-plugin",
  "description": "To work with DX Package Dependcies",
  "version": "1.3.0",
  "author": "java.rabota@gmail.com",
  "bugs": "https://github.com/baksale/sfdx-dependency-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2",
    "any-dependency-tree": "2.2.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12.12.8",
    "chai": "^4",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "nyc": "^14",
    "rimraf": "^3.0.0",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/bin"
  ],
  "homepage": "https://github.com/baksale/sfdx-dependency-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "dependency": {
        "description": "Commands to work with sfdx package dependencies."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "baksale/sfdx-dependency-plugin",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
